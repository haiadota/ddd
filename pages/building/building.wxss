.title{
  font-size: 34rpx;
  color: #000;
  font-family: Arial, Helvetica, sans-serif;
}
.item-men{
  height: 140rpx;
  width: 33%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  color: #000;
}
.spacing{
  letter-spacing: 2rpx;
  color: #000;
}
.circle{
  color: #fff;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  position: relative;
  background-color: #bbb;
}
.percentImg{
  position: absolute;
  left: 0;
  height: 106rpx;
  width: 80rpx;
}
.percentTxt{
  width: 100%;
  position: absolute;
}